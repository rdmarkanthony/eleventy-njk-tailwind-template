{
  "name": "eleventy-njk-tailwind-template",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pnpm dev",
    "dev": "concurrently \"pnpm run watch:css\" \"pnpm run serve\"",
    "serve": "eleventy --serve",
    "build": "eleventy",
    "watch:css": "tailwindcss -i ./src/assets/css/style.css -o ./public/assets/css/style.css --watch",
    "build:css": "tailwindcss -i ./src/assets/css/style.css -o ./public/assets/css/style.css --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.2.0",
    "@tailwindcss/postcss": "^4.2.0",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.0"
  }
}
